{
    "builds": [
      {
        "src": "FitmeBackend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "FitmeFrontend/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "FitmeBackend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "FitmeFrontend/dist/$1"
      }
    ],
    "headers": [
      {
        "source": "/manifest.webmanifest",
        "headers": [
          { "key": "Content-Type", "value": "application/manifest+json" }
        ]
      }
    ]
  }
  